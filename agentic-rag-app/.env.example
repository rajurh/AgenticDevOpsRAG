# Copy to .env and fill in your values
# Embedding endpoint (Azure OpenAI deployment embeddings URL)
AZURE_OPENAI_EMBEDDING_URL=<replace_with_your_embedding_endpoint>
# Chat (chat completions) endpoint (Azure OpenAI deployment chat URL)
AZURE_OPENAI_CHAT_URL=<replace_with_your_chat_endpoint>
# API key for the Azure OpenAI instance
AZURE_OPENAI_KEY=<replace_with_your_api_key>
# How many docs to retrieve
TOP_K=3
